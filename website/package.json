{
  "name": "@gen-design/website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "worker-dev": "TRIGGER_ENV=development npx trigger.dev@beta dev",
    "build": "next build",
    "postinstall": "npx prisma generate",
    "//server-only": "The following scripts are only for server use",
    "vercel-build": "npx prisma migrate deploy && next build",
    "ssh": "ssh ubuntu@5.78.129.100",
    "ssh-db-tunnel": "ssh -L 5433:localhost:5432 ubuntu@5.78.129.100",
    "start": "next start",
    "start-app": "pm2 start --time --name 'app' 'yarn start'",
    "stop-all": "pm2 ls | grep online && pm2 stop all && pm2 delete all || echo \"No processes to stop\"",
    "start-all": "yarn start-app && yarn tunnel && yarn tunnel-https",
    "tunnel": "pm2 start --time --name 'tunnel' 'sudo socat TCP-LISTEN:80,fork TCP:localhost:3000'",
    "tunnel-https": "pm2 start --time --name 'tunnel-https' 'sudo socat OPENSSL-LISTEN:443,reuseaddr,cert=/etc/letsencrypt/live/vibeflow.sh/fullchain.pem,key=/etc/letsencrypt/live/vibeflow.sh/privkey.pem,verify=0,fork TCP:localhost:3000'",
    "update-envs": ". ~/.bashrc",
    "deploy": "git pull && yarn install && yarn build && npx prisma migrate deploy && yarn stop-all && yarn update-envs && yarn start-all && pm2 save",
    "old-deploy": "npx prisma migrate deploy && vercel",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "next lint",
    "worker-deploy": "npx trigger.dev@beta deploy -e prod -l debug --skip-typecheck",
    "stripe-test": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
    "update-logo": "yarn convert-icon && yarn convert-svg",
    "convert-icon": "convert public/favicon.png -resize 32x32 public/favicon.ico",
    "convert-svg": "npx @svgr/cli --icon --typescript --replace-attr-values \"#000=currentColor\" src/components/Logo.svg > ../shared/Logo.tsx",
    "dump-db": "mkdir -p db_backups && dotenv -e .env -- bash -c 'PGPASSWORD=$PRODUCTION_DB_PASSWORD pg_dump -h $PRODUCTION_DB_HOST -p $PRODUCTION_DB_PORT -U $PRODUCTION_DB_USER $PRODUCTION_DB_NAME -F c -b -v -f ./db_backups/db_backup-$(date +%Y-%m-%d-%H%M).dump'",
    "restore-db": "pg_restore --clean --if-exists --no-owner --no-privileges --no-acl -d audiowaveai_prod_6 $(ls -t db_backups/db_backup-*.dump | head -n1)",
    "run-prod-script": "dotenv -e .env -- bash -c 'POSTGRES_PRISMA_URL=$PRODUCTION_DB_URL POSTGRES_URL_NON_POOLING=$PRODUCTION_DB_URL tsx $0 $@'"
  },
  "browserslist": "defaults, not ie <= 11",
